<section class="h-100">
    <div class="row">
      <div class="col-md-4 bg-warning text-dark" style="overflow:scroll; height:650px;">
      <div class="col-md-12">
    <%= image_tag("KEDAICODINGLOGO-01.png", :alt => "KedaiCoding Logo", class:"rounded mx-auto d-block top-50", style:"width:40%; height:40%") %>
      </div>
  <h5><%= t('.sign_up') %></h5>
  <div class="container px-5">
    <%= render 'devise/registrations/form' %>
  </div>
  <div class="my-2">
          <%= render 'devise/shared/links' %>
        </div>
        <div class="my-2 d-flex justify-content-center" style="font-size:18pt;">
          <%= link_to raw('<i class="fa fa-twitter-square"></i>'), new_password_path(resource_name),class:'socmed mx-2' %>
          <%= link_to raw('<i class="fa fa-facebook-square"></i>'), new_password_path(resource_name),class:'socmed mx-2' %>
          <%= link_to raw('<i class="fa fa-instagram"></i>'), new_password_path(resource_name),class:'socmed mx-2' %>
          <%= link_to raw('<i class="fa fa-youtube-square"></i>'), new_password_path(resource_name),class:'socmed mx-2' %>
        </div>
        <div class="d-flex justify-content-center">
          <%= link_to 'Terms of Use', new_password_path(resource_name),class:'foot-link-color mx-2' %>
          |
          <%= link_to 'Help Center', new_password_path(resource_name),class:'foot-link-color mx-2' %>
          |
          <%= link_to 'Contact Us', new_password_path(resource_name),class:'foot-link-color mx-2' %>
        </div>
        <div class="d-flex justify-content-center">
          KedaiCoding 2018 &copy; All Rights Reserved
        </div>
      </div>
      <div class="col-md-8 bg-login-img">

      </div>
    </div>
</section>